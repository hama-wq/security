{
    "compilerOptions": {
      "target": "ES2017",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react-jsx"
    },
    "include": ["next-env.d.ts", "**/*.js", "**/*.jsx", ".next/types/**/*.js"],
    "exclude": ["node_modules"]
  }
  